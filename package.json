{
    "name": "bento-blog-block",
    "version": "1.0.0",
    "description": "Bento Blog Block for WordPress",
    "scripts": {
        "build": "npm run build:post && npm run build:image && npm run build:video && npm run build:link && npm run build:map && npm run build:social && npm run build:text && npm run build:container && npm run build:gravatar",
        "start": "npm run start:post && npm run start:image && npm run start:video && npm run start:link && npm run start:map && npm run start:social && npm run start:text && npm run start:container && npm run build:gravatar",
        "watch": "npm run watch:post && npm run watch:image && npm run watch:video && npm run watch:link && npm run watch:map && npm run watch:social && npm run watch:text && npm run watch:container && npm run build:gravatar",
        "clean": "rm -rf includes/blocks/*/build",
        "build:image": "wp-scripts build --webpack-src-dir=includes/blocks/image/src --output-path=includes/blocks/image/build",
        "start:image": "wp-scripts start --webpack-src-dir=includes/blocks/image/src --output-path=includes/blocks/image/build",
        "watch:image": "wp-scripts start --webpack-src-dir=includes/blocks/image/src --output-path=includes/blocks/image/build --hot",
        "build:video": "wp-scripts build --webpack-src-dir=includes/blocks/video/src --output-path=includes/blocks/video/build",
        "start:video": "wp-scripts start --webpack-src-dir=includes/blocks/video/src --output-path=includes/blocks/video/build",
        "watch:video": "wp-scripts start --webpack-src-dir=includes/blocks/video/src --output-path=includes/blocks/video/build --hot",
        "build:link": "wp-scripts build --webpack-src-dir=includes/blocks/link/src --output-path=includes/blocks/link/build",
        "start:link": "wp-scripts start --webpack-src-dir=includes/blocks/link/src --output-path=includes/blocks/link/build",
        "watch:link": "wp-scripts start --webpack-src-dir=includes/blocks/link/src --output-path=includes/blocks/link/build --hot",
        "build:map": "wp-scripts build --webpack-src-dir=includes/blocks/map/src --output-path=includes/blocks/map/build",
        "start:map": "wp-scripts start --webpack-src-dir=includes/blocks/map/src --output-path=includes/blocks/map/build",
        "watch:map": "wp-scripts start --webpack-src-dir=includes/blocks/map/src --output-path=includes/blocks/map/build --hot",
        "build:post": "wp-scripts build --webpack-src-dir=includes/blocks/post/src --output-path=includes/blocks/post/build",
        "start:post": "wp-scripts start --webpack-src-dir=includes/blocks/post/src --output-path=includes/blocks/post/build",
        "watch:post": "wp-scripts start --webpack-src-dir=includes/blocks/post/src --output-path=includes/blocks/post/build --hot",
        "build:social": "wp-scripts build --webpack-src-dir=includes/blocks/social/src --output-path=includes/blocks/social/build",
        "start:social": "wp-scripts start --webpack-src-dir=includes/blocks/social/src --output-path=includes/blocks/social/build",
        "watch:social": "wp-scripts start --webpack-src-dir=includes/blocks/social/src --output-path=includes/blocks/social/build --hot",
        "build:text": "wp-scripts build --webpack-src-dir=includes/blocks/text/src --output-path=includes/blocks/text/build",
        "start:text": "wp-scripts start --webpack-src-dir=includes/blocks/text/src --output-path=includes/blocks/text/build",
        "watch:text": "wp-scripts start --webpack-src-dir=includes/blocks/text/src --output-path=includes/blocks/text/build --hot",
        "build:container": "wp-scripts build --webpack-src-dir=includes/blocks/container/src --output-path=includes/blocks/container/build",
        "start:container": "wp-scripts start --webpack-src-dir=includes/blocks/container/src --output-path=includes/blocks/container/build",
        "watch:container": "wp-scripts start --webpack-src-dir=includes/blocks/container/src --output-path=includes/blocks/container/build --hot",
        "build:gravatar": "wp-scripts build --webpack-src-dir=includes/blocks/gravatar/src --output-path=includes/blocks/gravatar/build",
        "start:gravatar": "wp-scripts start --webpack-src-dir=includes/blocks/gravatar/src --output-path=includes/blocks/gravatar/build",
        "watch:gravatar": "wp-scripts start --webpack-src-dir=includes/blocks/gravatar/src --output-path=includes/blocks/gravatar/build --hot",
        "build:header": "wp-scripts build --webpack-src-dir=includes/blocks/header/src --output-path=includes/blocks/header/build",
        "start:header": "wp-scripts start --webpack-src-dir=includes/blocks/header/src --output-path=includes/blocks/header/build",
        "watch:header": "wp-scripts start --webpack-src-dir=includes/blocks/header/src --output-path=includes/blocks/header/build --hot"
    },
    "dependencies": {
        "@wordpress/block-editor": "^9.0.0",
        "@wordpress/blocks": "^12.0.0",
        "@wordpress/components": "^19.0.0",
        "@wordpress/element": "^4.0.0",
        "@wordpress/i18n": "^4.0.0",
        "js-sha256": "^0.11.0",
        "leaflet": "^1.9.4",
        "react": "^17.0.2",
        "react-beautiful-dnd": "^13.1.1",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^17.0.2",
        "stream-browserify": "^3.0.0",
        "uuid": "^11.0.3",
        "vm-browserify": "^1.1.2"
    },
    "devDependencies": {
        "@wordpress/scripts": "^30.3.0",
        "ajv": "^8.17.1",
        "css-loader": "*",
        "mini-css-extract-plugin": "*",
        "sass-loader": "*"
    }
}
